{
    "name": "verbb/zen",
    "description": "Manage content across multiple environments with ease.",
    "type": "craft-plugin",
    "version": "1.0.4",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "zen"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/zen/issues?state=open",
        "source": "https://github.com/verbb/zen",
        "docs": "https://github.com/verbb/zen",
        "rss": "https://github.com/verbb/zen/commits/v2.atom"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0",
        "mistralys/text-diff": "^2.0",
        "nystudio107/craft-plugin-vite": "^4.0.0",
        "verbb/base": "^2.0.0",
        "wa72/htmlpagedom": "^2.0 || ^3.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.0.0",
        "vlucas/phpdotenv": "^3.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "verbb\\zen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "verbb\\zentests\\": "tests/"
        }
    },
    "extra": {
        "name": "Zen",
        "handle": "zen",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/zen/craft-4/CHANGELOG.md",
        "class": "verbb\\zen\\Zen"
    }
}
